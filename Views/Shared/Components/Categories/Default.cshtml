@using Bitirme_Projesi.Entities
@model IEnumerable<Category> //category

<ul class="list-group">
        <li class="list-group-item">
                <a class="nav-link text-dark" asp-controller="Home" asp-action="Index" asp-route-categorySlug="">Tüm Ürünler</a>
        </li>

        @foreach (var item in Model)
        {
                <li class="list-group-item">
                        <a class="nav-link text-dark" asp-controller="Home" asp-action="Index" asp-route-categorySlug="@item.CategoryName">@item.CategoryName</a>
                </li>
        }
</ul>